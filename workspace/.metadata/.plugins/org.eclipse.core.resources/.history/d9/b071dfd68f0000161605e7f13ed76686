
public class Soldier extends Ennemy{
	
	private int hitpoint;
	private int lives;

		super.hitpoint=points;
		super.lives=life;
	
	


	public int getHitpoint() {
		return hitpoint;
	}

	public int getLives() {
		return lives;
	}

	public void setHitpoint(int hitpoint) {
		this.hitpoint = hitpoint;
	}

	public void setLives(int lives) {
		this.lives = lives;
	}

}
